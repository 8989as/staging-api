    "type": "library",
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^10.0",
        "bagisto/bagisto": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Webkul\\PhoneAuth\\": "src/"
        }
    },
    "extra": {
